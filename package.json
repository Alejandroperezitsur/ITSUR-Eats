{
  "name": "itsur-eats-monorepo",
  "version": "6.0.0",
  "private": true,
  "workspaces": [
    "backend-api",
    "admin-panel",
    "mobile-app"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:admin\"",
    "dev:api": "npm run dev --workspace=backend-api",
    "dev:admin": "npm run dev --workspace=admin-panel",
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
